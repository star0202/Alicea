// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Log {
  id String @id @unique

  channel         String
  ignoredChannels IgnoredChannel[]
  ignoredUsers    IgnoredUser[]
}

model IgnoredChannel {
  id String @id

  Log   Log?    @relation(fields: [logId], references: [id])
  logId String?
}

model IgnoredUser {
  id String @id

  Log   Log?    @relation(fields: [logId], references: [id])
  logId String?
}

model Welcome {
  id String @id @unique

  user String?
  bot  String?
}

model Clean {
  id String @id @unique

  channels CleanChannel[]
}

model CleanChannel {
  id String @id @unique

  clean   Clean?  @relation(fields: [cleanId], references: [id])
  cleanId String?
}
